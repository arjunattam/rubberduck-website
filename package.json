{
  "name": "mercury-website",
  "description": "Code that powers rubberduck.io",
  "version": "1.0.0",
  "author": "Arjun Attam",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "devicon": "https://github.com/konpa/devicon",
    "gatsby": "^1.9.223",
    "gatsby-link": "^1.6.38",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-react-helmet": "^2.0.6",
    "gatsby-plugin-sharp": "^1.6.46",
    "gatsby-remark-copy-linked-files": "^1.5.35",
    "gatsby-remark-images": "^1.5.65",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "reactstrap": "5.0.0-beta"
  },
  "keywords": ["gatsby"],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public/",
    "build": "npm run clean && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gsbackup":
      "gsutil -m cp -r gs://www.rubberduck.io/dmgs/* gs://rubberduck-dmgs-backup",
    "gsrestore":
      "gsutil -m cp -r gs://rubberduck-dmgs-backup/* gs://www.rubberduck.io/dmgs/ && npm run gspermissions",
    "gsclean":
      "npm run gsbackup && gsutil -m rm \"gs://www.rubberduck.io/**\" && npm run gsrestore",
    "deploy": "gsutil -m cp -r public/* gs://www.rubberduck.io",
    "gspermissions":
      "gsutil -m acl ch -u AllUsers:R \"gs://www.rubberduck.io/**\"",
    "push": "npm run build && npm run deploy && npm run gspermissions"
  },
  "devDependencies": {
    "prettier": "^1.11.1"
  }
}
